{
    "version": 2,
    "builds": [
        {
            "src": "api/dist/**/*",
            "use": "@vercel/static-build"
        },
        {
            "src": "api/**/*",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/dist/$1"
        }
    ]
}